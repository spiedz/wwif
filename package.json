  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:optimized": "npm run optimize-content && cross-env NODE_ENV=production next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:full-optimize": "npm run clean && npm run optimize-content && npm run optimize-images && npm run generate-sitemap && cross-env NODE_ENV=production next build",
