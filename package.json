    "optimize-content": "node scripts/optimize-content.js",
    "optimize-images": "node scripts/optimize-images.js",
    "quality-check": "node scripts/content-quality-checker.js",
    "fix-content-formatting": "node scripts/fix-content-formatting.js",
    "build:faster": "npm run fix-content-formatting && npm run build:optimized",
    "clean": "rimraf .next",
    "upgrade:next": "npm install next@latest react@latest react-dom@latest",
    "upgrade:check": "npx @next/codemod@latest next-11-to-12",
    "build:report": "cross-env ANALYZE=true next build",
    "dev:turbo": "next dev --turbo"
